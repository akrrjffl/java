package day03;

import java.util.Scanner;

public class 복습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		// 양수, 음수, 0 출력
		Scanner sc = new Scanner(System.in);
		System.out.print("숫자입력>");
		int num = sc.nextInt();
		
	    if(num >0) {
	    	System.out.println("양수");
	    }else if(num<0) {
	    		System.out.println("음수");
	    	}else if(num==0);
	    }
		
	}


