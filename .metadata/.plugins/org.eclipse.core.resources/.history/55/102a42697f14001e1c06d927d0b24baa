		// 1~100까지 3의배수 갯수 출력
		// 1~100까지 3의배수, 5의배수, 3과5의 배수 갯수 출력
		Scanner sc = new Scanner(System.in);
		int cnt = 0;
		int cnt1 = 0;
		int cnt2 = 0;
		for(int i=0; i<=100; i++) {
			if(i % 3 == 0 && i % 5 == 0) {
				cnt++;
			}else if(i % 3 == 0) {
				cnt1++;
			}else if(i % 5 == 0) {
				cnt2++;
			}
		}
		System.out.println(cnt);
		System.out.println(cnt1);
		System.out.println(cnt2);

	}

}








